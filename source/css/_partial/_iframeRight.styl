#iframe-right
  width 88%
  float right
  height 100%
  overflow-y auto
  background-color #222222
  .top-col-div
    height 26px
    margin-bottom 10px
  .top-col
    width 100%
    height 25px
    position fixed
    background-color #3c3f41
    border 1px solid #4b4b4b
    ul li
      float left
      padding-right 10px
      padding-left 10px
      line-height 27px
      color #C9C9C9
      font-size 13px
      border-right 0.5px solid #4B4B4B
    lihover
      background-color #4B6EAF
      cursor pointer
    .top-col-x
      font-size 14px
      color #61696D
      padding-left 7px
  .left-col
    width 3%
    float left
    min-height 1200px
    border-right 1px solid #555555
    ul li
      font-size 11px
      color #f8f8f2
      padding-bottom 7px
      margin-left 6px
  .right-col
    width 96%
    float right
    min-height 1200px
    font-size 15px
    .right-body
      #main .main-post
        margin-top 20px
        .main-post-h3
          color #CCCCCC
        .main-post-date
          font-size 12px
          margin-left 20px
        .main-post-content
          width 88%
          margin-top 20px
          margin-bottom 20px
          background-color #3C3F41
          padding 20px
          color #DCDCDC
          font-size 13px
          border-radius 5px




